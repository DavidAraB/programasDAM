package examne1ev;
import java.util.Scanner;
public class Ejer2 {

	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);
		int dni,n;
		String letra ,le = ("TRWAGMYFPDXBNJZSQVHLCKE");
		boolean correcto = false;
		while(correcto == false) {
			dni = teclado.nextInt();
			letra = teclado.next();
			n=dni%23;
			if(letra.equals(le.charAt(n))) {
				correcto = true;
			}
		}

	}

}
