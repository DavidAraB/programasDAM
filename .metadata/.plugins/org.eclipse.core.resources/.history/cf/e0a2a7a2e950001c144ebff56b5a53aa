package examne1ev;
import java.util.Scanner;
public class Ejer1 {

	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);
		int n = teclado.nextInt(),i,ii;
		for(i=0;i<n;++i) {
			for(ii=0;ii<=i;++ii) {
			System.out.print("*");
			}
			System.out.println();
		}

	}

}
